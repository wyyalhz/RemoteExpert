{
  "branches": [
    "master"
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "RemoteExpert-*-windows.zip",
            "label": "Windows Release"
          },
          {
            "path": "RemoteExpert-*-linux.tar.gz",
            "label": "Linux Release"
          }
        ]
      }
    ]
  ]
}
